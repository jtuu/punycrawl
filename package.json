{
  "name": "punycrawl",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "clean": "rm build/*.js || true",
    "tsc": "node_modules/typescript/bin/tsc",
    "fix_module_names": "find build -name '*.js' -exec sed -i '/import/s/\";/.js\";/' {} \\;",
    "build": "npm run tsc -- --build src/tsconfig.json && npm run fix_module_names || true",
    "server": "pushd build; python3 -m http.server; popd"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
